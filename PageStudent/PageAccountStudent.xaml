<Page x:Class="Wpf_PR_12_2_Pisarev.PageStudent.PageAccountStudent"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Wpf_PR_12_2_Pisarev.PageStudent"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Аккаунт студента">

    <Grid Background="White">
        <StackPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Left">
            <TextBlock Text="Имя студента" FontSize="20">
                <ListView x:Name="s1" Margin="18"/>
            </TextBlock>
            <TextBlock Text="Логин студента"  FontSize="20">
                <ListView x:Name="s2" Margin="18"/>
            </TextBlock>
        </StackPanel>
    </Grid>
</Page>
